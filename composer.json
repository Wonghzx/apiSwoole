{
  "name": "apiswoole/apiswoole",
  "description": "PHP 常驻内存型PHP框架 ",
  "license": "MIT",
  "keywords": [
    "message queue"
  ],
  "version": "1.0.0",
  "authors": [
    {
      "name": "Wongzx"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "jasongrimes/paginator": "^1.0",
    "vlucas/phpdotenv": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "Conf\\": "Conf",
      "Http\\": "Http"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
